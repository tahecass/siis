<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="IndicadorMapper">
	<resultMap id="resMapIndicador" type="IndicadorAlias">
		<result property="nombre" column="NOMBRE" />
		<result property="secuenciaIndicador" column="SECUENCIA_INDICADOR" />
		<result property="usuario.secuencia" column="SECUENCIA_USUARIO" />
		<result property="objetivoEsperado" column="OBJETIVO_ESPERADO" />
		<result property="objetivoReal" column="OBJETIVO_REAL" />
		<result property="desviacion" column="DESVIACION" />
		<result property="unidad.secuenciaUnidad" column="SECUENCA_UNIDAD" />
		<result property="documentacion" column="DOCUMENTACION" />
	
	</resultMap>


	<select id="listarIndicadores" resultMap="resMapIndicador"
		resultType="java.util.Map">
		SELECT 
		  "INDICADORES"."SECUENCIA_INDICADOR", 
		  "INDICADORES"."SECUENCIA_USUARIO", 
		  "INDICADORES"."OBJETIVO_ESPERADO", 
		  "INDICADORES"."OBJETIVO_REAL", 
		  "INDICADORES"."DESVIACION", 
		  "INDICADORES"."SECUENCA_UNIDAD", 
		  "INDICADORES"."NOMBRE", 
		  "INDICADORES"."DOCUMENTACION"
		FROM 
		  public."INDICADORES"
	</select>


</mapper>
	